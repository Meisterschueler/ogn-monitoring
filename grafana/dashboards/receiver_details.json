        "name": "Reboot",
        "target": {
          "editorMode": "code",
          "format": "table",
          "limit": 100,
          "matchAny": false,
          "rawQuery": true,
          "rawSql": "SELECT\n\tsq.ts AS time,\n\t'reboot' AS event\nFROM (\n\tSELECT\n\t\tts,\n\t\tsrc_call,\n\t\tsenders_messages,\n\t\tLAG(senders_messages) OVER (PARTITION BY src_call ORDER BY ts) AS senders_messages_prev\n\tFROM statuses\n\tWHERE \n\t\t$__timeFilter(ts) \n\t\tAND src_call = '$sel_receiver'\n) AS sq\nWHERE\n  sq.senders_messages IS NOT NULL AND sq.senders_messages_prev > sq.senders_messages\n\t",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
