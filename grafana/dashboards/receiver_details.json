        "name": "Server Change",
        "target": {
          "editorMode": "code",
          "format": "time_series",
          "limit": 100,
          "matchAny": false,
          "rawQuery": true,
          "rawSql": "SELECT\n\tsq.ts AS time,\n\t'server changed' AS event,\n\tsq.receiver_prev || ' to ' || sq.receiver AS desc\nFROM (\n\tSELECT\n\t\tts,\n\t\tsrc_call,\n\t\treceiver,\n\t\tLAG(receiver) OVER (PARTITION BY src_call ORDER BY ts) AS receiver_prev\n\tFROM statuses\n\tWHERE\n\t\t$__timeFilter(ts)\n\t\tAND src_call = '$sel_receiver'\n\tORDER BY ts\n) AS sq\nWHERE\n\tsq.receiver_prev IS NOT NULL AND sq.receiver_prev != sq.receiver",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
