            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\n\ts.name,\n\ts.address AS \"flarm:address\",\n  \n\tCASE WHEN s.is_stealth IS NULL THEN 'UNKNOWN' WHEN s.is_stealth THEN 'NO' ELSE 'YES' END AS \"flarm:stealth\",\n\tCASE WHEN s.is_notrack IS NULL THEN 'UNKNOWN' WHEN s.is_notrack THEN 'NO' ELSE 'YES' END AS \"flarm:track\",\n\tCASE WHEN d.is_notrack IS NULL THEN 'UNKNOWN' WHEN d.is_notrack THEN 'NO' ELSE 'YES' END AS \"ddb:track\",\n\tCASE WHEN d.is_noident IS NULL THEN 'UNKNOWN' WHEN d.is_noident THEN 'NO' ELSE 'YES' END AS \"ddb:ident\",\n\t\n\ts.software_version AS \"flarm:sw\",\n\tto_char(f.expiry_date, 'YYYY-MM-DD') AS \"sw:expiry\",\n\tCASE\n\t\tWHEN f.expiry_date IS NULL THEN ''\n\t\tWHEN f.expiry_date - NOW() >= INTERVAL'1 year' THEN 'GOOD'\n\t\tWHEN f.expiry_date - NOW() > INTERVAL'6 months' THEN 'MINOR'\n\t\tWHEN f.expiry_date - NOW() > INTERVAL'1 month' THEN 'MAJOR'\n\t\tWHEN f.expiry_date - NOW() > INTERVAL'1 day' THEN 'CRITICAL'\n\t\tELSE 'EXPIRED'\n\tEND AS \"sw:state\",\n\t'0x' || s.hardware_version AS \"flarm:hw\"\nFROM senders AS s\nINNER JOIN ddb AS d ON s.address = d.address\nLEFT JOIN flarm_expiry AS f ON f.version = s.software_version\nWHERE\n\ts.is_notrack = FALSE AND d.is_notrack = False\n\tAND d.is_noident = False\nORDER BY s.name\n",
